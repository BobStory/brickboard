generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DB_URL")
}

model mod_cases {
  case_id         BigInt  @id
  type            Int?
  user_id         BigInt?
  author_id       BigInt?
  reason          String? @db.Text
  proof           String? @db.Text
  channel_id      BigInt?
  timestamp_now   Int?
  timestamp_end   Int?
  log_message_url String? @db.Text
  dm_message_url  String? @db.Text
}

model brickboard_users {
  id          BigInt  @id
  username    String  @db.Text
  global_name String? @db.Text
  avatar      String  @db.Text
  locale      String  @db.Text
}

model brickboard_auth {
  id            BigInt  @id
  access_token  String? @db.VarChar(255)
  refresh_token String? @db.VarChar(255)
  scope         String? @db.Text
  session_id    String? @db.VarChar(64)
}